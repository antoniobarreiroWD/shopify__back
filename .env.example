
# Server Configuration
PORT=your_port_number_here # Example: 3000
NODE_ENV=your_environment_here # Example: development, production, staging

# Frontend URL for CORS and redirects
FRONTEND_URL=your_frontend_url_here # Example: http://localhost:3001

# Session security
SESSION_SECRET=your_session_secret_here # Example: my-super-secret-key-123

# Shopify API Configuration
SHOPIFY_SHOP_NAME=your_shop_name_here # Example: my-store.myshopify.com
SHOPIFY_ACCESS_TOKEN=your_access_token_here # Example: shpat_123456789abcdef
SHOPIFY_API_VERSION=your_api_version_here # Example: 2024-01

# CORS Configuration
CORS_ORIGIN=your_cors_origin_here # Example: http://localhost:3001


